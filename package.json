    "@typescript-eslint/parser": "^8.38.0",
    "allure-playwright": "^3.2.2",
    "eslint": "^9.32.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-playwright": "^2.2.2",
    "husky": "^9.1.7",
    "prettier": "^3.6.2",
    "start-server-and-test": "^2.1.0",
